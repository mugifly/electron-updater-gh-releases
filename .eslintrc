{
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
		"no-unused-vars": 0,
		"no-console": 0,
		"no-empty": 0
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
		"mocha": true
    },
    "extends": "eslint:recommended"
}
